#include <stdio.h>
#include <stdlib.h>
#include "LinkedList.h"
#include "utn.h"

int main(void)
{
	setbuf(stdout,NULL);

	LinkedList* pArrayArticulos = ll_newLinkedList();
	int option = 0;
	char path[51];

	while(option != 5)
	{
		printf("\n1-Cargar un archivo y abrirlo");
		utn_getInt(&option,"\nElija una opcion: " ,"\nError\n",1,10,3);

		switch(option)
		{
		case 1:
			controller_loadData(path,pArrayArticulos);
			break;

		}
	}



	return EXIT_SUCCESS;
}
